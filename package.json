{
  "name": "render-with-react17",
  "version": "0.1.4",
  "description": "A simple util component to render components incompatible with React 18.x.",
  "keywords": [
    "react",
    "react-library",
    "react-component",
    "react-renderer",
    "react17"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Damian Cyntler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyntler/render-with-react17.git"
  },
  "scripts": {
    "start": "rimraf ./dist && tsc --watch",
    "build": "rimraf ./dist && tsc",
    "lint": "eslint ./src",
    "prettier": "prettier --check ./src",
    "test": "jest",
    "release": "npm run lint && npm run prettier && npm run build && release-it"
  },
  "devDependencies": {
    "@cyntler/eslint-config": "^0.0.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "release-it": "^15.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}
